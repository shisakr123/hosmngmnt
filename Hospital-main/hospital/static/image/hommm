    <section class="col-sm-12">
                  <a id="main-content"></a>
                    <h1 class="display-none page-header">Healthcare Forum</h1>
                                                  	  <div>
		  <div class="to-table clearfix">
						<div class="col-xs-12 col-sm-8 col-md-9 to-tablecell">
								  <div id="herosliderforum">
					  <div class="region region-herosliderhome">
    <section id="block-views-forum-banner-block" class="block block-views clearfix">

      
  <div class="view view-forum-banner view-id-forum_banner view-display-id-block view-dom-id-c144224cd484d31a24bb7e470b7d9a8a">
        
  
  
      <div class="view-content">
        <div class="views-row views-row-1 views-row-odd views-row-first views-row-last">
      
  <div class="views-field views-field-field-hero-image-para">        <div class="field-content"><ul><li><div class="entity entity-paragraphs-item paragraphs-item-hero-image">
  <div class="content">
    <div class="field field-name-field-hero-image-style-pg field-type-iss field-label-hidden"><div class="field-items"><div class="field-item even"><img typeof="foaf:Image" class="img-responsive" src="https://www.opengroup.org/sites/default/files/styles/no_style/public/healthcare-slides1_0_0.jpg?itok=SY1kX1QV" width="685" height="200" alt="" /></div></div></div>  </div>
